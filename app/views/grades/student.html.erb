<p id="notice"><%= notice %></p>

<h2>Welcome <%=@student.name%>
  <p></p>
<h1>Grades</h1>

<table>
  <thead>
    <tr>
      <th> Assignment name </th>
      <th> Score </th>
      <th> Date Completed <th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grades.each do |grade| %>
      <tr>
        <td><%= grade.assignment_name %></td>
        <td><%= grade.score %></td>
        <td><%= grade.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
