<p id="notice"><%= notice %></p>


<h1>Grades</h1>

<h2>Welcome <%=@student.name%>
<table>
  <tr> <%=@student.name%> </tr>
  <thead>
    <tr>
      <th> Student Name </th>
      <th> Assignment name </th>
      <th> Score </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grades.each do |grade| %>
      <tr>
        <td><%= grade.assignment_name %></td>
        <td><%= grade.score %></td>
        <td><%= link_to 'Show', grade %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
